<?xml version="1.0" encoding="UTF-8"?>
<!-- This file generated by Podcastgen -->
<rss xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" version="2.0">
  <channel>
    <title><%= @ch.title %></title>
    <link><%= @ch.url %></link>
    <description><%= @ch.description %></description>
    <itunes:summary><%= @ch.description %></itunes:summary>
    <% unless @ch.language.blank? %>
    <language><%= @ch.language %></language>
    <% end %>
    <% unless @ch.copyright.blank? %>
    <copyright><%= @ch.copyright %></copyright>
    <% end %>
    <% unless @ch.subtitle.blank? %>
    <itunes:subtitle><%= @ch.subtitle %></itunes:subtitle>
    <% end %>
    <% unless @ch.author.blank? %>
    <itunes:author><%= @ch.author %></itunes:author>
    <% end %>
    <% unless @ch.owner_name.nil? %>
    <itunes:owner>
      <itunes:name><%= @ch.owner_name %></itunes:name>
      <itunes:email><%= @ch.owner_email %></itunes:email>
    </itunes:owner>
    <% end %>
    <% unless @ch.image_url.blank? %>
    <itunes:image href="<%= @ch.image_url %>"/>
    <% end %>
    <% unless @ch.categories.blank? %>
    <itunes:category text="<%= @ch.categories %>"/>
    <% end %>
    <itunes:explicit><%= @ch.explicit ? 'Yes' : 'No' %></itunes:explicit>
    <generator>Podcastgen</generator>
    <% unless @ch.keywords.blank? %>
    <itunes:keywords><%= @ch.keywords %></itunes:keywords>
  <% end %>
  <% @ch.items.each{|item| %>
    <item>
      <title><%= item.title %></title>
      <itunes:author><%= item.author %></itunes:author>
      <itunes:subtitle><%= item.subtitle %></itunes:subtitle>
      <itunes:summary><%= item.summary %></itunes:summary>
      <itunes:image href="<%= item.image_url %>"/>
      <enclosure url="<%= item.url %>" length="<%= item.file_size %>" type="audio/mp3"/>
      <link><%= item.url %></link>
      <guid isPermaLink="false"><%= item.uuid %></guid>
      <pubDate><%= item.pub_date.to_formatted_s(:rfc822) %></pubDate>
      <itunes:duration><%= item.duration %></itunes:duration>
    <% unless item.keywords.blank? %>
      <itunes:keywords><%= item.keywords %></itunes:keywords>
    <% end %>
    </item>
  <% } %>
  </channel>
</rss>
